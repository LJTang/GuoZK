.information_Items{
  height:40px;
  line-height:40px;
  margin:0 20rpx;
}
/*************** 两个按钮 ***************/
.twoBtn view{
  height:40px;
  line-height:40px;
  background:#ec5958;
  margin-left:20rpx;
  border-radius:5px;
  text-align: center;
  color:#FFFFFF;
}
.twoBtn view:first-child{width:200rpx;}
.twoBtn view:last-child{ width:490rpx;}
.twoBtn button{
  margin: 0;
  background:#ec5958;
  border:none;
  height:40px;
  line-height:40px;
  color: #FFFFFF;
  font-size:16px;
}
.btn_ShareInformation{
  background:#ec5958;
  color:#fff;
  margin:0 10px;
  height:40px;
  line-height:40px;
  border-radius:5px;
  text-align: center;
}