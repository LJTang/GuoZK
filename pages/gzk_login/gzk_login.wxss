.loin-Head{
  width:100%;
  height:30px;
  line-height:30px;
  text-align: center;
  background:#ec5958;
  color:#fff;
  font-size: 14px;
}
.log_Phone{
  height:40px;
  line-height: 40px;
  margin:0 20rpx;
  border-bottom:1px solid #d9d9d9;
}
.log_Phone image{
  width:30rpx;
  height:20px;
  line-height:40px;
  vertical-align:top;
  margin-top:10px;
  margin-left:10rpx;
  margin-right:10rpx;
}
.log_Phone input{
  display:inline-block;
  width:640rpx;
  height:40px;
  line-height: 40px;
  margin-left:20rpx;
}

.log_Verification_Code{
  display: flex;
  flex-direction: row;
  height:40px;
  line-height: 40px;
  margin:0 20rpx;
  padding:10px 0;
  border-bottom:1px solid #d9d9d9;
}
.verification_Code_Input{
  display:inline-block;
  width:465rpx;
  height:40px;
  line-height: 40px;
}
.verification_Code_Input image{
  width:38rpx;
  height:20px;
  line-height:40px;
  vertical-align:top;
  margin-top:10px;
  margin-left:10rpx;
  margin-right:10rpx;
}
.verification_Code_Input input{
  display: inline-block;
  width:385rpx;
  height:40px;
  line-height: 40px;
  margin-left:20rpx;
}

.verification_Code_Btn{
  margin-left:20rpx;
  width:225rpx;
  height:40px;
  line-height: 40px;
  text-align: center;
  border-radius:5px;
}
.doNotOperate{
  background:#999999;
}
.operational{
  background:#ec5958;
  color:#FFFFFF;
}
.log_Prompt{
  margin-left:30rpx;
  margin-right:20rpx;
}
.log_Btn{
  height:40px;
  line-height: 40px;
  margin:20px 20rpx;
  text-align: center;
  background:#ec5958;
  color:#FFFFFF;
  border-radius:5px;
}


/** tck **/
.pup-up-shadow{
  background:#313131;
  opacity:0.7;
}
.gzk_UserType{
  background:#FFFFFF;
  margin-top:-140px;
  margin-left:100rpx;
  width:550rpx;
  height:280px;
  border-radius:5px;
  position: absolute;
  top:50%;
}
.gzk_UserType-Title{
  height: 50px;
  line-height: 50px;
  text-align: center;
}
.pup-up-Clear{
  position: absolute;
  top:2px;
  right:2px;
}
.gzk_UserType-Prompt{
  font-size:32rpx;
  padding:10px 0;
  text-align: center;
  background:#fbe9df;
  color:#d14d00;
}
.gzk_UserType-Select view{
  height: 50px;
  line-height: 50px;
  border-bottom:1px solid #d9d9d9;
  text-align: center;
  color:#00a0e9;
}
