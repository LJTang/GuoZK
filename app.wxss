/*** search ***/
.weui-search-bar {
  position:relative;
  padding:8px 10px;
  display:-webkit-box;
  display:-webkit-flex;
  display:flex;
  box-sizing:border-box;
  background-color:#ec5958;
}
.weui-search-bar__form {
  position:relative;
  -webkit-box-flex:1;
  -webkit-flex:auto;
  flex:auto;
  border-radius:5px;
  background:#fff;
}
.weui-search-bar__box {
  position:relative;
  padding-left:30px;
  padding-right:30px;
  width:100%;
  box-sizing:border-box;
  z-index:1;
  height:40px;
}
.weui-icon-search_in-box {
  position:absolute;
  left:10px;
  top:13px;
}
.weui-search-bar__input {
  height:40px;
  line-height:40px;
  font-size:16px;
}

.weui-icon-clear {
  position:absolute;
  top:7px;
  right:0;
  padding: 7px 8px;
  font-size: 0;
}

.weui-search-bar__label {
  position:absolute;
  top:0;
  right:0;
  bottom:0;
  left:0;
  z-index:2;
  border-radius:3px;
  text-align:center;
  color:#9b9b9b;
  background:#fff;
  line-height:28px;
}
.weui-search-bar__text {
  display:inline-block;
  font-size:14px;
  vertical-align:middle;
}

.weui-icon-search {
  margin-right:8px;
  font-size:inherit;
  margin-top:5px;
}

.weui-search-bar__cancel-btn {
  margin-left:10px;
  line-height:40px;
  color:#FFFFFF;
  white-space:nowrap;
  width:200rpx;
  background:#f0ca4d;
  text-align:center;
  border-radius:5px;
  font-size:16px;
}

.searchbar-result{
  margin-top: 0;
  font-size: 14px;
}
.searchbar-result:before{
  display: none;
}
.weui-cell{
  padding: 12px 15px 12px 35px;
}


